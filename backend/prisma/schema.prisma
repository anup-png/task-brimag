datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Order {
  orderId     String   @id @default(uuid())
  productName String
  quantity    Int
  status      Status   @default(PENDING)
  createdAt   DateTime @default(now())
}

enum Status {
  PENDING
  PROCESSING
  COMPLETED
}
